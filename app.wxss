/**app.wxss**/
/*noinspection CssInvalidPropertyValue*/
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0rpx;
    box-sizing: border-box;
}

.index{
    align-items: center;
    display: flex;
    flex-direction: column;
}

.row {
    display: block;
}

.avatar {
    width: 192rpx;
    height: 192rpx;
    border-radius: 50%;
}

